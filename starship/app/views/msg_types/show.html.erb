<h1>Listing messages for: <%= @msgtype.type_desc %></h1>
<table class="ruler">


<% for message in @msgs_to_show %>
	<tr id="msgtr">
	<td><%= link_to message.id, message %></td>
	<td><%=h message.sender %></td>
	<td><%=h message.subject %></td>
	</tr>
<% end %> 


</table>
<%= will_paginate @msgs_to_show %>
